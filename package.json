{
  "name": "svgx",
  "version": "0.4.1",
  "description": "is a universal SVG renderer",
  "main": "build/svgx.js",
  "scripts": {
    "build": "babel src/ -d build/",
    "check-deps": "npm outdated",
    "git_add-f": "git add -f build/",
    "gh-pages": "git subtree --prefix gh-pages push origin gh-pages",
    "lint": "standard",
    "preversion": "npm run build; npm run git_add-f",
    "postversion": "git push origin v${npm_package_version}; npm publish; npm run push",
    "push": "git push origin master; npm run gh-pages",
    "test": "NODE_PATH=src babel-tape-runner test/components/Svg.js",
    "example": "NODE_PATH=src babel-node example.js"
  },
  "pre-commit": [
    "lint",
    "test",
    "check-deps"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fibo/SVGx.git"
  },
  "keywords": [
    "svg",
    "universal",
    "react"
  ],
  "author": {
    "name": "Gianluca Casati",
    "url": "http://g14n.info"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fibo/SVGx/issues"
  },
  "homepage": "http://npm.im/svgx",
  "devDependencies": {
    "babel-tape-runner": "^2.0.1",
    "pre-commit": "^1.1.2",
    "standard": "^7.0.1",
    "tape": "^4.5.1"
  },
  "dependencies": {
    "babel-cli": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "cheerio": "^0.20.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2"
  }
}
